{
    "name": "vscode-ido",
    "displayName": "vscode-ido",
    "description": "Emacs ido mode for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.ido"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.ido",
                "title": "ido"
            },
            {
                "command": "extension.ido-go-up-directory",
                "title": "ido-go-up-directory"
            },
            {
                "command": "extension.ido-select-item",
                "title": "ido-select-item"
            }
        ],
        "keybindings": [
            {
                "command": "extension.ido-go-up-directory",
                "key": "ctrl+l",
                "when": "idoMode"
            },
            {
                "command": "extension.ido-select-item",
                "key": "ctrl+j",
                "when": "idoMode"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/vscode": "^1.37.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "typescript": "^3.3.1",
        "tslint": "^5.12.1",
        "vscode-test": "^1.0.2"
    }
}
